desc=Formats the output application logs containing typical words associated to priorities
# Error
regexp=.*(Error|Critical|Errore|Erro|Err).*
colours=red
count=stop
# Warning
regexp=.*(Alarm|Allarme|Warn|Warning).*
colours=yellow
count=stop
# Message
regexp=.*(Info|Message|Messaggio).*
colours=green
count=stop
# Debug
regexp=.*(Debug|Dbg).*
colours=blue
count=stop
